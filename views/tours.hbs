<section>
    <div class="well">
        <h1>Тури</h1>
    </div>
    <table class="table table-stripped">
        <thead>
            <tr>
                <th>Країни</th>
                <th>Дата відправки</th>
                <th>Вартість</th>
            </tr>
        </thead>
        <tbody>
            {{#each tours}}
            <tr>
                <td>{{country}}</td>
                <td>{{date}}</td>
                <td>{{price}}</td>
            </tr>
            {{/each}}
        </tbody>
    </table>
</section>